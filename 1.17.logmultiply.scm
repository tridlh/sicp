(define (lmultiply a b)
  (cond ((= a 0) 0)
        ((= b 0) 0)
        ((even? b) (lmultiply (double a) (halve b)))
        (else (+ a (lmultiply a (- b 1))))))
(define (even? n)
  (= (remainder n 2) 0))
(define (double n)
  (+ n n))
(define (halve n)
  (/ n 2))
(lmultiply 0 100)
(lmultiply 100 0)
(lmultiply 1 20)
(lmultiply 20 1)
(lmultiply 13 100)
(lmultiply 14 100)
(lmultiply 13 101)
(lmultiply 14 101)
